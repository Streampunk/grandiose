{
  "name": "grandiose",
  "version": "0.0.5",
  "description": "Node.JS native bindings to the Newtek NDI SDK.",
  "main": "index.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexK-99/grandiose.git"
  },
  "keywords": [
    "Newtek",
    "NDI",
    "network",
    "device",
    "interface"
  ],
  "author": "Streampunk Media <furnace@streampunk.media> (http://www.streampunk.media)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AlexK-99/grandiose"
  },
  "homepage": "https://github.com/AlexK-99/grandiose#readme",
  "gypfile": true,
  "dependencies": {
    "node-pre-gyp": "0.13.0"
  },
  "binary": {
    "module_name": "grandiose",
    "module_path": "./build/{configuration}/",
    "remote_path": "{version}",
    "package_name": "{platform}-{arch}.tar.gz",
    "host": "https://github.com/AlexK-99/grandiose/releases/download/"
  }
}
